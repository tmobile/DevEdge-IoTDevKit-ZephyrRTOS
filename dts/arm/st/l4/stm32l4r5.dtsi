#include <mem.h>
#include <st/l4/stm32l4p5.dtsi>

/delete-node/ &sdmmc2;
/delete-node/ &sram0;

/ {
	sram0: memory@20000000 {
		reg = <0x20000000 DT_SIZE_K(640)>;
	};

	soc {
		rtc@40002800 {
			bbram: backup_regs {
				compatible = "st,stm32-bbram";
				st,backup-regs = <32>;
				status = "disabled";
			};
		};
	};
};
