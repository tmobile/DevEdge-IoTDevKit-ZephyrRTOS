/ {
	stm-vbat {
		compatible = "st,stm32-vbat";
		ratio = <3>;
		io-channels = <&adc1 18>;
		status = "okay";
	};
};

&adc1 {
	vbat-channel = <18>;
	vref-mv = <3300>;
};
